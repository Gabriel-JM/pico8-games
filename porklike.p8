-- initialization

function _init()
	-- directions in x and y
	dirs_x=split"-1,1,0,0,1,1,-1,-1"
	dirs_y=split"0,0,-1,1,-1,1,1,-1"
	
	dpal=split[[
		0,1,1,2,1,13,6,4,
		4,9,3,13,1,13,14
	]]
	
	mob_sprs=split"240,192"
	mob_atk=split"1,1"
	mob_hp=split"5,2"
	
	frame_time=0
		
	startgame()
end

function _draw()
	draw_func()
	draw_windows()
	upd_hp_box()
	check_fade()
end

function _update60()
	frame_time+=1
	update_func()
	anim_floaters()
end

function startgame()
	fade_perc=1
	
	btn_buffer=-1
	
	-- monsters lists
	mob_list={}
	dead_mobs={}

	-- player
	plyr=add_mob(1,1,1)
	plyr_timer=0
	
	add_mob(2,2,5)
	add_mob(2,6,8)
	add_mob(2,7,12)
	
	window_list={}
	talk_window=nil
	hp_box=add_window(
		5,5,28,13,{"♥5/5"}
	)
	
	-- list of floating text
	floaters={}
	
	update_func=update_game
	draw_func=draw_game
end
