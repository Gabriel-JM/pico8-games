-- ui

function add_window(
	x,
	y,
	width,
	height,
	text
)
	return add(window_list,{
		x=x,
		y=y,
		width=width,
		height=height,
		text=text
	})
end

function draw_windows()
	for_each(
		window_list, 
		function(w)
			local w_x,w_y,
				w_width,w_height=
					w.x,w.y,w.width,w.height
					
			-- draw three rectangles
			for n=0,2 do
				draw_rect(
					w_x+n,
					w_y+n,
					w_width-n*2,
					w_height-n*2,
					n==1 and 6 or 0
				)
			end
			
			w_x+=4
			w_y+=4
			
			clip(
				w_x,
				w_y,
				w_width-8,
				w_height-8
			)
	
			for_each(
				w.text,
				function(text)
					print(text,w_x,w_y,6)
					w_y+=6
				end
			)
		end
	)
end

