__lua__
-- initialization

function _init()
	update_func=update_game
	draw_func=draw_game
	
	directions_x={-1,1,0,0}
	directions_y={0,0,-1,1}
	
	frame_time=0
	player_sprite=240
		
	startgame()
end

function _draw()
	draw_func()
	draw_windows()
end

function _update60()
	frame_time+=1
	update_func()
end

function startgame()
	btn_buffer=-1

	player_x=1
	player_y=2
	player_ox=0
	player_oy=0
	player_sox=0
	player_soy=0
	player_animation=nil
	player_is_flipped=false
	player_timer=0
	
	window_list={}
	
	add_window(32,64,64,24,{
		"hello world",
		"this is line 2"
	})
end
-->8
-- updates

function update_game()
	do_btn_buff()
	
	btn_action()
	btn_buffer=-1
end

function update_player_turn()
	do_btn_buff()

	player_timer=min(
		player_timer+0.125,
		1
	)
	
	player_animation()
	
	if player_timer==1 then
		update_func=update_game
	end
end

function update_gameover()

end

function walk_animation()
	local timer=1-player_timer
	player_ox=player_sox*timer
	player_oy=player_soy*timer
end

function bump_animation()
	local timer=player_timer>0.5
		and 1-player_timer
		or player_timer
	
	player_ox=player_sox*timer
	player_oy=player_soy*timer
end

function do_btn_buff()
	if btn_buffer==-1 then
		btn_buffer=get_btn()
	end
end

function get_btn()
	for i=0,5 do
		if btnp(i) then
			return i
		end
	end
	return -1
end

function btn_action()
	if btn_buffer<0 then return end
	
	if btn_buffer<4 then
		move_player(
			directions_x[btn_buffer+1],
			directions_y[btn_buffer+1]
		)
		return
	end
	
	-- menu button
end

