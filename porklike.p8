-- draws

function draw_game()
	cls(0)
	map()
	
	draw_sprite(
		get_frame(player_sprite,4),
		player_x*8 + player_ox,
		player_y*8 + player_oy,
		10,
		player_is_flipped
	)
	for_each(
		mob_list,
		function (mob)
			draw_sprite(
				get_frame(mob.sprite,4),
				mob.x*8,
				mob.y*8,
				10,
				false
			)
		end
	)
end

function draw_gameover()

end
