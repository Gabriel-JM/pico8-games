-- tools

function get_frame(
	start_point,
	length
)
	return start_point+(
		flr(frame_time/15)%length
	)
end

function draw_sprite(
	sprite_num,
	x_position,
	y_position,
	color_num,
	is_flipped
)
	--makes black not transparent
	palt(0,false)
	
	--turn gray to another color
	pal(6,color_num)
	spr(
		sprite_num,
		x_position,
		y_position,
		1,1,
		is_flipped
	)
	pal()
end

function draw_rect(
	x,
	y,
	width,
	height,
	color_num
)
	rectfill(
		x,
		y,
		x+width-1,
		y+height-1,
		color_num
	)
end

function for_each(table,cb)
	for i=1,#table do
		cb(table[i],i)
	end
end

