-- gameplay

function move_player(dx,dy)
	local dest_x,dest_y=
		player_x+dx,player_y+dy
	local tile=mget(dest_x,dest_y)
	
	if dx<0 then
		player_is_flipped=true
	elseif dx>0 then
		player_is_flipped=false
	end
	
	if fget(tile,0) then
		-- wall
		player_sox,player_soy=dx*8,dy*8
		player_ox,player_oy=0,0
					
		player_animation=bump_animation
		if fget(tile,1) then
			trigger_bump(
				tile,
				dest_x,
				dest_y
			)
		end
	else
		sfx(63)
		player_x+=dx
		player_y+=dy
				
		player_sox,player_soy=-dx*8,-dy*8
		player_ox,player_oy=
			player_sox,player_soy
		
		player_animation=walk_animation
	end
	
	player_timer=0
	update_func=update_player_turn
end

function trigger_bump(
	tile,
	dest_x,
	dest_y
)
	if tile==7 or tile==8 then
		-- vase
		sfx(59)
		mset(dest_x,dest_y,1)
	elseif tile==10 or tile==12 then
		-- chest
		sfx(61)
		mset(dest_x,dest_y,tile-1)
	elseif tile==13 then
		-- door
		sfx(62)
		mset(dest_x,dest_y,1)
	end
end

