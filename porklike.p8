-- ui

function add_window(
	x,
	y,
	width,
	height,
	text
)
	return add(window_list,{
		x=x,
		y=y,
		width=width,
		height=height,
		text=text
	})
end

function draw_windows()
	for_each(
		window_list,
		function (w)
			local w_x,w_y,
				w_width,w_height=
					w.x,w.y,w.width,w.height
			
			draw_rect(
				w_x,
				w_y,
				w_width,
				w_height,
				0
			)
			rect(
				w_x+1,
				w_y+1,
				w_x+w_width-2,
				w_y+w_height-2,
				6
			)
			
			w_x+=4
			w_y+=4
			
			clip(
				w_x,
				w_y,
				w_width-8,
				w_height-8
			)
			for_each(
				w.text,
				function (text)
					print(text,w_x,w_y,6)
					w_y+=6
				end
			)
			clip()
			
			if w.duration then
				w.duration-=1
				
				if w.duration<=0 then
					local diff=w_height/4
					w.y+=diff/2
					w.height-=diff
					
					if w_height<3 then
						del(window_list,w)
					end
				end
			else
				if w.btn then
					outline_print(
						"❎",
						w_x+w_width-15,
						w_y-0.9+sin(time()),
						6,
						0
					)
				end
			end
		end
	)
end

function show_message(
	text,
	duration
)
	local width=(#text+2)*4+7
	local window=add_window(
		63-width/2, --x
		50, --y
		width,
		13, -- height
		{" "..text}
	)
	window.duration=duration
end

function show_message(text)
	talk_window=add_window(
		16, -- x
		50, -- y
		94, -- width
		#text*6+7, --height
		text
	)
	talk_window.btn=true
end

