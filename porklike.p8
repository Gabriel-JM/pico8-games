-- tools

function get_frame(
	start_point,
	length
)
	return start_point+(
		flr(frame_time/15)%length
	)
end

function draw_sprite(
	sprite_num,
	x_position,
	y_position,
	color_num,
	is_flipped
)
	--makes black not transparent
	palt(0,false)
	
	--turn gray to another color
	pal(6,color_num)
	spr(
		sprite_num,
		x_position,
		y_position,
		1,1,
		is_flipped
	)
	pal()
end

function draw_rect(
	x,
	y,
	width,
	height,
	color_num
)
	rectfill(
		x,
		y,
		x+max(width-1,0),
		y+max(height-1,0),
		color_num
	)
end

function for_each(
	table,
	call_back
)
	for i=1,#table do
		call_back(table[i],i)
	end
end

function outline_print(
	text,
	x,
	y,
	color1,
	color2
)
	for i=1,8 do
		print(
			text,
			x+directions_x[i],
			y+directions_y[i],
			color2
		)
	end
	print(text,x,y,color1)
end

