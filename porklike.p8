-- draws

function draw_game()
	cls(0)
	map()
	
	for_each(dead_mobs,
		function (mob)
			if sin(time()*8)>0 then
				draw_mob(mob)
			end
			
			mob.dur-=1
			
			if mob.dur<=0 then
				del(dead_mobs,mob)
			end
		end
	)
	
	for_each(mob_list,function (m)
		if (m==plyr) return	
		draw_mob(m)
	end)
	
	draw_mob(plyr)
	
	for_each(floaters,
		function (flt)
			outline_print(
				flt.text,
				flt.x,
				flt.y,
				flt.color_num,
				0
			)
		end
	)
end

function draw_gameover()
	cls(2)
	print("u ded",50,50,7)
end

function draw_mob(mob)
	local colr=10
		
	if mob.flash>0 then
		mob.flash-=1
		colr=7
	end

	draw_sprite(
		get_frame(mob.sprt),
		mob.x*8+mob.ox,
		mob.y*8+mob.oy,
		colr,
		mob.is_flipped
	)
end
