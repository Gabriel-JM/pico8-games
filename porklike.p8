pico-8 cartridge // http://www.pico-8.com
version 34
__lua__
function _init()
	update_func=update_game
	draw_func=draw_game
	
	startgame()
end

function _draw()
	draw_func()
end

function _update()
	update_func()
end

function startgame()
	player_x=3
	player_y=6
	player_sprite_num=240
end
-->8
-- updates

function update_game()
	if btnp(⬅️) then
		player_x-=1
	end
	if btnp(➡️) then
		player_x+=1
	end
	if btnp(⬆️) then
		player_y-=1
	end
	if btnp(⬇️) then
		player_y+=1
	end
end

function update_gameover()

end
-->8
-- draws

function draw_game()
	cls(0)
	map()
	
	-- makes black not transparent
	palt(0,false)
	
	-- turn gray(6) to yellow(10)
	pal(6,10)
	spr(
		player_sprite_num,
		player_x * 8,
		player_y * 8
	)
	pal()
end

function draw_gameover()

end
